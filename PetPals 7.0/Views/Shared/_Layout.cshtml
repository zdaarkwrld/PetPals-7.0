<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PetPals</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/PetPals_7._0.styles.css" asp-append-version="true" />
    
</head>
<body>
    <header> 
        <nav style="background-color:#64b53a;border:0px " class=" navbar navbar-expand-sm navbar-toggleable-sm navbar-light  border-bottom box-shadow mb-3" >
            <div class="container-fluid">
                <a style="color:white !important;" class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">PetPals</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <style>

                </style>

                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a style="color:white !important;"class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a style="color:white !important;"class="nav-link text-dark" asp-area="" asp-controller="Services" asp-action="Index">Services</a>
                        </li>
                    
                        <li class="nav-item">
                            <a style="color:white !important;"class="nav-link text-dark" asp-area="" asp-controller="Messages" asp-action="Index">Talk To Community</a>
                        </li>
                        <li class="nav-item">
                            <a style="color:white !important;"class="nav-link text-dark" asp-area="" asp-controller="Schedules" asp-action="Index">Book Service</a>
                        </li>
                        <li class="nav-item">
                            <a style="color:white !important;"class="nav-link text-dark" asp-area="" asp-controller="Admins" asp-action="Index">Admin</a>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div  class="container w-100">
        <main  role="main" class="w-100 pb-3"> 
            @RenderBody()
        </main>
    </div>

    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
   
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
